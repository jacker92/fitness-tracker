{
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#101085",
        "titleBar.inactiveBackground": "#101085"
    },
    "editor.tabSize": 4,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.next": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.format.enable": true
}
