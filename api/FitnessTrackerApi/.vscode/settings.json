{
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#00554e",
        "titleBar.inactiveBackground": "#00554e"
    },
    "editor.tabSize": 2,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/.next": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.format.enable": true,
}
